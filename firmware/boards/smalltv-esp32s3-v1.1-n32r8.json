{
  "build": {
    "arduino":{
      "ldscript": "esp32s3_out.ld",
      "memory_type": "qio_opi"
    },
    "partitions": "partitions_32MB.csv",
    "filesystem": "fatfs",
    "core": "esp32",
    "extra_flags": [
      "-DBOARD_HAS_PSRAM",
      "-DARDUINO_USB_MODE=1",
      "-DARDUINO_USB_CDC_ON_BOOT=1",
      "-DARDUINO_SMALLTV_BOARD",
      "-DARDUINO_SMALLTV_BOARD_REV=1"
    ],
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "hwids": [
      [
        "0X303A",
        "0x1001"
      ]
    ],
    "mcu": "esp32s3",
    "variant": "esp32s3"
  },
  "connectivity": [
    "wifi",
    "bluetooth"
  ],
  "debug": {
    "openocd_target": "esp32s3.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "name": "SmallTV ESP32-S3 Board v1.1 (N32R8)",
  "upload": {
    "flash_size": "32MB",
    "maximum_ram_size": 327680,
    "maximum_size": 33554432,
    "require_upload_port": true,
    "speed": 921600
  },
  "url": "https://github.com/Yevgeniy-Olexandrenko/smalltv-esp32s3-board",
  "vendor": "Yevgeniy Olexandrenko"
}
