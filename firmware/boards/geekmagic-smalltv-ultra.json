{
    "build": {
        "ldscript": "eagle.flash.4m1m.ld",
        "core": "esp8266",
        "extra_flags": [
            "-DESP8266",
            "-DARDUINO_ARCH_ESP8266",
            "-DARDUINO_GEEKMAGIC_SMALLTV_ULTRA",
            "-DNO_VINSENSE",
            "-DNO_SELFRES",
            "-DNO_BUTTON",
            "-DNO_LED",
            "-DNO_SDCARD",
            "-DPIN_LCD_RES=2",
            "-DPIN_LCD_CS=-1",
            "-DPIN_LCD_SCL=14",
            "-DPIN_LCD_SDA=13",
            "-DPIN_LCD_DC=0",
            "-DPIN_LCD_BL=5",
            "-DLCD_BL_INV",
            "-DNO_AUDIO",
            "-DNO_MICROPHONE",
            "-DNO_TOUCH0",
            "-DNO_TOUCH1",
            "-DNO_TOUCH2",
            "-DNO_TOUCH3",
            "-DNO_EXPANSION"
        ],
        "f_cpu": "80000000L",
        "f_flash": "40000000L",
        "flash_mode": "dio",
        "mcu": "esp8266",
        "variant": "generic"
    },
    "connectivity": [
        "wifi"
    ],
    "frameworks": [
        "arduino"
    ],
    "name": "GeekMagic SmallTV-Ultra",
    "upload": {
        "maximum_ram_size": 81920,
        "maximum_size": 4194304,
        "require_upload_port": true,
        "resetmethod": "nodemcu",
        "speed": 921600
    },
    "url": "https://github.com/GeekMagicClock/smalltv-ultra",
    "vendor": "GeekMagic"
}
